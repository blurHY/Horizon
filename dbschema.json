          "UNSIGNED INT"
        ],
        [
          "site_addr",
          "varchar(100)"
        ],
        [
          "json_id",
          "INTEGER REFERENCES json (json_id)"
        ]
      ],
      "indexes": [
        "create index query_main on main (id,url,title)"
      ],
      "schema_changed": 6
