{
  "db_name": "Horizon",
  "db_file": "data/Horizon.db",
  "version": 2,
  "maps": {
    "data_main.+.json": {
      "to_table": [
        "main"
      ]
    },
    "data_keywords.+.json": {
      "to_table": [
        "keywords"
      ]
    },
    "data_phrases.+.json": {
      "to_table": [
        "phrases"
      ]
    },
    "data_relationship.+.json": {
      "to_table": [
        {
          "node": "relationship",
          "table": "relationship",
          "key_col": "parent",
          "val_col": "sub"
        }
      ]
    },
    "data_zites.+.json": {
      "to_table": [
        "zites"
      ]
    },
    "users/.+/data.json": {
      "to_table": [
        "search_zite",
        "main_spider"
      ]
    },
    "users/.+/content.json": {
      "to_keyvalue": [
        "cert_user_id"
      ]
    },
    "names.*json": {
      "to_table": [
        {
          "node": "names",
          "table": "names",
          "key_col": "domain",
          "val_col": "address"
        }
      ]
    }
  },
  "tables": {
    "search_zite": {
      "cols": [
        [
          "header",
          "TEXT NOT NULL"
        ],
        [
          "site_address",
          "TEXT NOT NULL"
        ],
        [
          "site_type",
          "TEXT NOT NULL"
        ],
        [
          "date_added",
          "INTEGER NOT NULL"
        ],
        [
          "json_id",
          "INTEGER REFERENCES json (json_id) NOT NULL"
        ]
      ],
      "indexes": [
      ],
      "schema_changed": 5
    },
    "main_spider": {
      "cols": [
        [
          "header",
          "TEXT NOT NULL"
        ],
        [
          "site_address",
