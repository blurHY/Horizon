        ]
      ],
      "indexes": [
      ],
      "schema_changed": 4
    },
    "main": {
      "cols": [
        [
          "id",
          "UNSIGNED INT primary key"
        ],
        [
          "url",
          "VARCHAR(700)"
        ],
        [
          "imgcount",
          "UNSIGNED INT"
        ],
        [
          "title",
          "VARCHAR(90)"
        ],
        [
          "state",
          "UNSIGNED INT"
        ],
        [
          "priority",
          "UNSIGNED INT"
        ],
        [
